{
  "name": "rap-tap-app",
  "displayName": "ラップ、タップ、アップ 🎶",
  "displayShortName": "ラップ🎤",
  "description": "リアルタイム の 競演 と 参加型観戦 で 音楽を最高に楽しむ",
  "applicationHost": "https://riotz.works",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf .nuxt dist static/sw.js static/sitemap.xml*",
    "setup": "yarn install --ignore-optional",
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "generate": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "deploy": "gh-pages -d dist -b master -m 'Update Site [ci skip]'",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "firebase": "^7.13.1",
    "nuxt": "^1.4.2",
    "qrcode": "^1.4.4",
    "skyway-js": "^1.4.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^2.1.0",
    "@nuxtjs/vuetify": "^0.5.7",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.25.0",
    "all-contributors-cli": "^6.14.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.3.1",
    "ts-loader": "3.5.0",
    "typescript": "^3.8.3"
  },
  "homepage": "https://github.com/riotz-works/rap-tap-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/riotz-works/rap-tap-app.git"
  },
  "bugs": {
    "url": "https://github.com/riotz-works/rap-tap-app/issues"
  },
  "author": "Riotz.works (https://riotz.works)",
  "license": "MIT"
}
